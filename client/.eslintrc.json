{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": ["standard-with-typescript","prettier","plugin:@next/next/recommended"],
  "plugins" : ["prettier"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
      "linebreak-style": ["off"],
      "eol-last": "off"
  }
}